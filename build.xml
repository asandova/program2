<project default="compile">

	<target name="clean">
		<delete>
			<fileset dir=".">
				<include name="*.class"/>
			</fileset>
		</delete>
	</target>

	<target name="compile">
		<javac srcdir="." destdir="." includeantruntime="false"/>
	</target>

	<target name="run">
		<java classname="WebServer" fork="true"></java>
	</target>
</project>
